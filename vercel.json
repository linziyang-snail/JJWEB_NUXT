{
    "rewrites": [
      { "source": "/(.*)/$", "destination": "/$1" },
      { "source": "/_vercel/image", "destination": "/_ipx/$1" }
    ],
    "redirects": [
      { "source": "/www/(.*)", "destination": "/$1", "permanent": true }
    ],
    "headers": [
      {
        "source": "/_ipx/image",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }
  